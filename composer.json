{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "5.1.*",
		"league/commonmark": "0.10.0",
		"erusev/parsedown-extra": "0.7.0",
		"symfony/browser-kit": "2.7.*"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1",
		"sami/sami": "3.0.*"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php-cli artisan clear-compiled",
			"php-cli artisan optimize"
		],
		"post-update-cmd": [
			"php-cli artisan clear-compiled",
			"php-cli artisan optimize"
		],
		"post-create-project-cmd": [
			"php-cli -r \"copy('.env.example', '.env');\"",
			"php-cli artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"prefer-stable": true
}
